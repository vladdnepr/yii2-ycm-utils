{
    "name": "vladdnepr/yii2-ycm-utils",
    "description": "YCM - Yii 2 Content Management module Utilities",
    "keywords": ["yii2", "yii2-ycm", "yii2-ycm-utils", "admin", "administration", "module", "extension", "utils", "utilities"],
    "homepage": "https://github.com/vladdnepr/yii2-ycm-utils",
    "type": "yii2-extension",
    "license": "public domain",
    "authors": [
        {
            "name": "Vladislav Lyshenko",
            "email": "vladdnepr1989@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/vladdnepr/yii2-ycm-utils/issues?state=open",
        "source": "https://github.com/vladdnepr/yii2-ycm-utils"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "janisto/yii2-timepicker": "*",
        "kartik-v/yii2-grid": "~3.0",
        "kartik-v/yii2-krajee-base": "~1.7",
        "kartik-v/yii2-mpdf": "~1.0",
        "kartik-v/yii2-editable": "^1.7",
        "vladdnepr/trait-utils": "*",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-field-range": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "vladdnepr\\ycm\\utils\\": "src"
        }
    },
  "extra": {
      "bootstrap": "vladdnepr\\ycm\\utils\\Bootstrap"
  }
}
